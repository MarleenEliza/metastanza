      "stanza:required": true
    },
    {
      "stanza:key": "data-type",
      "stanza:example": "json",
      "stanza:description": "Type of data",
      "stanza:required": true
    },
    {
      "stanza:key": "width",
      "stanza:example": "800",
      "stanza:description": "Width"
    },
    {
      "stanza:key": "height",
      "stanza:example": "auto",
      "stanza:description": "Height"
